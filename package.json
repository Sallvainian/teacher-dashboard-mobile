{
  "name": "svelt-version",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">=20.18.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "npx vite dev",
    "build": "npx vite build",
    "build:analyze": "npx vite build --mode analyze",
    "preview": "npx vite preview",
    "prepare": "npx svelte-kit sync",
    "check": "npx svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "npx svelte-kit sync --tsconfig ./tsconfig.json --watch",
    "check:svelte": "npx svelte-check",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint --fix .",
    "validate": "npx run-s validate:quality lint check",
    "validate:quality": "node scripts/validate-code-quality.js",
    "format": "npx prettier --write \"src/**/*.{js,ts,svelte,css,html,json}\"",
    "format:check": "npx prettier --check \"src/**/*.{js,ts,svelte,css,html,json}\"",
    "build:release": "node scripts/create-release.js",
    "release": "npx run-s validate build:release",
    "test": "npx vitest run",
    "test:unit": "npx vitest watch",
    "test:coverage": "npx vitest run --coverage",
    "optimize": "npx run-s validate test build",
    "mobile:build": "npm run build && npx cap sync",
    "mobile:android": "npm run mobile:build && npx cap open android",
    "mobile:ios": "npm run mobile:build && npx cap open ios",
    "mobile:serve": "npx cap run android --livereload --external",
    "mobile:add-android": "npx cap add android",
    "mobile:add-ios": "npx cap add ios"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/types": "^7.27.3",
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.28.0",
    "@iconify/json": "^2.2.284",
    "@rollup/rollup-win32-x64-msvc": "^4.44.1",
    "@stylistic/eslint-plugin": "^4.4.1",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.21.5",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/language-server": "^0.14.23",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.8",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.0.3",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.5.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import-x": "^4.15.2",
    "eslint-plugin-svelte": "^3.7.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "glob": "^10.3.10",
    "globals": "^16.2.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.4.0",
    "rollup": "^4.43.0",
    "svelte": "^5.33.11",
    "svelte-check": "^4.2.1",
    "svelte-eslint-parser": "^1.2.0",
    "tailwindcss": "^3.4.17",
    "terser": "^5.40.0",
    "tsx": "^4.7.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0",
    "unplugin-icons": "^22.1.0",
    "vite": "^6.3.5",
    "vite-plugin-checker": "^0.9.3",
    "vitest": "^3.2.3"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.0",
    "@capacitor/app": "^7.0.1",
    "@capacitor/cli": "^7.4.0",
    "@capacitor/core": "^7.4.0",
    "@capacitor/haptics": "^7.0.1",
    "@capacitor/ios": "^7.4.0",
    "@capacitor/keyboard": "^7.0.1",
    "@capacitor/splash-screen": "^7.0.1",
    "@capacitor/status-bar": "^7.0.1",
    "@fontsource/inter": "^5.2.5",
    "@giphy/js-fetch-api": "^5.6.0",
    "@jridgewell/sourcemap-codec": "^1.5.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "caniuse-lite": "1.0.30001723",
    "cookie": "^1.0.2",
    "date-fns": "^4.1.0",
    "esbuild": "^0.25.5",
    "handsontable": "^15.3.0",
    "idb": "^8.0.3",
    "ionic-svelte": "^0.5.85",
    "ionicons": "^8.0.9",
    "pdfjs-dist": "^5.3.31",
    "strip-ansi": "^7.1.0",
    "uuid": "^11.1.0",
    "zod": "^3.25.46"
  },
  "packageManager": "npm@10.2.4",
  "overrides": {
    "glob": "^10.3.10",
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.5.0"
  },
  "optionalDependencies": {
    "@esbuild/win32-x64": "^0.25.5"
  },
  "trustedDependencies": [
    "core-js",
    "unrs-resolver"
  ]
}
